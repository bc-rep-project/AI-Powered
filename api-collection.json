              "mode": "raw",
              "raw": {
                "email": "test@example.com",
                "password": "password123"
              }
            }
          }
        }
      ]
    }
  ],
  "variable": [
    {
      "key": "baseUrl",
      "value": "https://ai-recommendation-api.onrender.com"
    }
  ]
} 